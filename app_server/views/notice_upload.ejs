<body>
  <div class="container" style="float: left;width:300px;height:750px;background-color: #e8e8e8;">
      <div class="col-sm-2">
<!-- As a link -->
   <nav class="navbar ">
    <a class="navbar" href="/" style="text-decoration:none;"><h2>Astas</h2></a>
   </nav>

        <div class="row">
            <div class="col-3">
              <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                <!-- <a class="nav-link active" id="v-pills-home-tab" data-toggle="pill" href="#notice-hod-home" role="tab">Home</a> -->
                <a class="nav-link" id="v-pills-profile-tab"  href="#notice-hod-profile" role="tab" >Profile</a>
                <a class="nav-link" id="v-pills-messages-tab"  href="notice-hod-messages" role="tab" >Messages</a>
                <a class="nav-link" id="v-pills-settings-tab"  href="#notice-hod-settings" role="tab" >Settings</a>
              </div>
            </div>
          </div>
        </div>
    </div> 

            
            <div class="container">
                <div class="row">
                    <div class="col-md-6 m-auto">
                        <h3>Choose the notice to be uploaded</h3>
                        <form action="/notice-data" method="POST" enctype="multipart/form-data">
                            <div class="custom-file mt-3 ">
                                <input type="file" class="custom-file-input" name="file">
                                <label for="file" class="custom-file-label">Choose File</label><br><br>
                                  <div class="form-group">
                                    <label for="NoticeHeading">Name</label>
                                    <input type="title" class="form-control" id="NoticeHeading"  placeholder="Enter the name to displayed" required><br>
                                    <label for="Description">Description</label>
                                    <input type="text" class="form-control" id="Description"  placeholder="Enter description about the notice" required>
                                  </div>
                                <input type="submit" value="submit" class="btn btn-block btn-primary">
                            </div>  
                        </form>
                    </div>
                </div>
            </div>


</body>
